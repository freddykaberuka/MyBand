{
    "env": {
        "browser": true,
        "es2021": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "quotes":0,
        "no-underscore-dangle":0,
        "indent":0,
        "no-multiple-empty-lines":0,
        "no-trailing-spaces":0,
        "padded-blocks":0,
        "import/named":0,
        "import/no-mutable-export":0,
        "import/extensions":0,
        "func-names":0,
        "no-restricted-syntax":0,
        "no-undef":0,
        "import/no-mutable-exports":0
    }
}
